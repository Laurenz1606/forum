{
  "name": "frontend",
  "version": "0.0.0",
  "description": "A simple forum for online communities. (frontend)",
  "repository": "https://github.com/Laurenz1606/forum.git",
  "author": "Laurenz1606 <laurenz1606 <laurenz.rausche@gmail.com>>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "scripts": {
    "build:clean": "rimraf dist",
    "build:compile": "npm run dev:format && npm run build:clean && tsc && vite build",
    "build:copy": "rimraf ../out/frontend/* && recursive-copy dist ../out/frontend/dist && recursive-copy ./deploy/index.js ../out/frontend/index.js && recursive-copy ./deploy/package.json ../out/frontend/package.json",
    "build:full": "npm run build:compile && npm run build:copy && npm run build:clean",
    "console:clear": "node -e \"console.clear()\"",
    "dev:format": "prettier --write --config ../.prettierrc.json src",
    "dev:remove": "rimraf node_modules yarn.lock package-lock.json",
    "dev:start": "npm run console:clear && cross-env NODE_ENV=development vite"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "daisyui": "^2.47.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "recursive-copy-cli": "^1.0.20",
    "rimraf": "^4.1.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
